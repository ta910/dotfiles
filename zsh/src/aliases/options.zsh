#### options.zsh
# options.zsh is aliases of command's options

# default
#---------------------------------
alias -g G='| grep'
alias -g GE='| grep -e'
alias -g L='| less'
alias -g H='| head'
alias -g T='| tail'
alias -g S='| sort'
alias -g W='| wc'
alias -g X='| xargs'
alias -g XK='| xargs kill -9'

# jq
#---------------------------------
alias -g J='| jq .'
alias -g JL='| jq length'
